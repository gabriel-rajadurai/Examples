<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.test.cooperfit_test.ProfileCooperfitFragment"
    android:background="#22262b">

    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/user"
        android:id="@+id/img_cfitProfilePic"/>

    <TextView
        android:id="@+id/tv_cfitName"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img_cfitProfilePic"
        android:textColor="@color/mdtp_white"
        android:textSize="16dp"
        android:text="Name : "/>

    <TextView
        android:id="@+id/tv_cfitEmail"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_cfitName"
        android:textColor="@color/mdtp_white"
        android:textSize="16dp"
        android:text="Email : "/>

    <TextView
        android:id="@+id/tv_cfitDOB"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_cfitEmail"
        android:textColor="@color/mdtp_white"
        android:textSize="16dp"
        android:text="Date of Birth : "/>

    <TextView
        android:id="@+id/tv_cfitGender"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_cfitDOB"
        android:textColor="@color/mdtp_white"
        android:textSize="16dp"
        android:text="Gender : "/>

</RelativeLayout>
